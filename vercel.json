{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "src/**/*",
            "node_modules/swagger-ui-dist/**",
            "node_modules/swagger-ui-express/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/api-docs(?:/.*)?",
        "dest": "src/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.ts"
      }
    ]
  }
  
